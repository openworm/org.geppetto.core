
<Lems>

    <Target component="sim_c302_A" />

    <!-- Include core NeuroML2 ComponentType definitions -->

    <Include url="https://raw.github.com/NeuroML/NeuroML2/master/NeuroML2CoreTypes/NeuroML2CoreTypes.xml"/>
    <Include file="/Simulation.xml"/>

    <!-- Including file with a <neuroml> root, a "real" NeuroML 2 file -->
    <Include url="https://raw.githubusercontent.com/openworm/org.geppetto.samples/master/LEMS/C302/c302_A.nml"/>
    

    <!-- End of NeuroML2 content -->
    

    <Simulation id="sim_c302_A" length="500ms" step="0.1ms" target="c302_A">

        <Display id="d0" title="c302_A: Membrane potential of a number of cells" timeScale="1ms" xmin="-50.0" xmax="550.0" ymin="-72" ymax="-48">
            
            <Line id="ADAL" quantity="ADAL/0/generic_iaf_cell/v" scale="1mV" color="#ecb097" timeScale="1ms" />
            <Line id="ADAR" quantity="ADAR/0/generic_iaf_cell/v" scale="1mV" color="#a03600" timeScale="1ms" />
            <Line id="BDUR" quantity="BDUR/0/generic_iaf_cell/v" scale="1mV" color="#732aef" timeScale="1ms" />
            <Line id="I1R" quantity="I1R/0/generic_iaf_cell/v" scale="1mV" color="#76e0d8" timeScale="1ms" />
            <Line id="I2L" quantity="I2L/0/generic_iaf_cell/v" scale="1mV" color="#b59d00" timeScale="1ms" />
            <Line id="PVDR" quantity="PVDR/0/generic_iaf_cell/v" scale="1mV" color="#5a37e9" timeScale="1ms" />
            
        </Display>

        
    </Simulation>


</Lems>
